{
  "name": "@postparrot/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.15.0",
    "pnpm": "10.9.0"
  },
  "packageManager": "pnpm@10.9.0",
  "engineStrict": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@postparrot/api": "workspace:*",
    "@postparrot/auth": "workspace:*",
    "@postparrot/db": "workspace:*",
    "@postparrot/ui": "workspace:*",
    "@postparrot/validators": "workspace:*",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/react-query": "catalog:",
    "@trpc/server": "catalog:",
    "geist": "^1.3.1",
    "next": "^14.2.28",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "superjson": "2.2.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@postparrot/eslint-config": "workspace:*",
    "@postparrot/prettier-config": "workspace:*",
    "@postparrot/tailwind-config": "workspace:*",
    "@postparrot/tsconfig": "workspace:*",
    "@types/node": "^22.14.1",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "jiti": "^2.4.2",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@postparrot/prettier-config"
}
